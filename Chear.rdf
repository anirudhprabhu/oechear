            <owl:Restriction>
                <owl:onProperty rdf:resource="&chear;calculatedFrom"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&chear;Height"/>
                            <rdf:Description rdf:about="&chear;Weight"/>
